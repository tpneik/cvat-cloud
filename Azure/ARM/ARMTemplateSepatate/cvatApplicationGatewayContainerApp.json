        "virtualNetworks_cvat_app_cae_infra_vnet_externalid": {
            "defaultValue": "/subscriptions/bb79d671-6949-4663-854e-14424d667454/resourceGroups/cvat-app-cae-infra-rg/providers/Microsoft.Network/virtualNetworks/cvat-app-cae-infra-vnet",
            "type": "String"
        },
        "publicIPAddresses_cvat_app_ag_public_ip_externalid": {
