                            "protocol": "Http",
                            "hostNames": [],
                            "requireServerNameIndication": false,
                            "customErrorConfigurations": []
                        }
                    },
                    {
                        "name": "lsn90",
                        "id": "[concat(resourceId('Microsoft.Network/applicationGateways', parameters('applicationGateways_cvat_app_ag_infra_name')), '/httpListeners/lsn90')]",
