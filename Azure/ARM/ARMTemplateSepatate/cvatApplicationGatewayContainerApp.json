                    }
                ],
                "routingRules": [],
                "probes": [
                    {
                        "name": "check",
                        "id": "[concat(resourceId('Microsoft.Network/applicationGateways', parameters('applicationGateways_cvat_app_ag_infra_name')), '/probes/check')]",
                        "properties": {
                            "protocol": "Https",
                            "host": "cvat-ui.ashygrass-f407c8e6.southeastasia.azurecontainerapps.io",
                            "path": "/",
                            "interval": 30,
                            "timeout": 30,
                            "unhealthyThreshold": 3,
                            "pickHostNameFromBackendHttpSettings": false,
                            "minServers": 0,
