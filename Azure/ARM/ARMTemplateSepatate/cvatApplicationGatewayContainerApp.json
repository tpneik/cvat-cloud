                                        "paths": [
                                            "/*"
                                        ],
                                        "backendAddressPool": {
                                            "id": "[concat(resourceId('Microsoft.Network/applicationGateways', parameters('applicationGateways_cvat_app_ag_infra_name')), '/backendAddressPools/frontend')]"
                                        },
                                        "backendHttpSettings": {
