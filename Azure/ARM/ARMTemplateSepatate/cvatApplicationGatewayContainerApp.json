                    },
                    {
                        "name": "port_90",
                        "id": "[concat(resourceId('Microsoft.Network/applicationGateways', parameters('applicationGateways_cvat_app_ag_infra_name')), '/frontendPorts/port_90')]",
                        "properties": {
                            "port": 90
