{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "managedEnvironments_cvat_test_env_name": {
            "defaultValue": "cvat-test-env",
            "type": "String"
        },
        "virtualNetworks_cvat_app_vnet_externalid": {
            "defaultValue": "/subscriptions/bb79d671-6949-4663-854e-14424d667454/resourceGroups/cvat-prod-rg/providers/Microsoft.Network/virtualNetworks/cvat-app-vnet",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.App/managedEnvironments",
            "apiVersion": "2025-02-02-preview",
            "name": "[parameters('managedEnvironments_cvat_test_env_name')]",
            "location": "Southeast Asia",
            "identity": {
                "type": "SystemAssigned"
            },
            "properties": {
                "vnetConfiguration": {
                    "internal": true,
                    "infrastructureSubnetId": "[concat(parameters('virtualNetworks_cvat_app_vnet_externalid'), '/subnets/cvat-app-subnet')]"
                },
                "appLogsConfiguration": {},
                "zoneRedundant": false,
                "kedaConfiguration": {},
                "daprConfiguration": {},
                "customDomainConfiguration": {},
                "workloadProfiles": [
                    {
                        "workloadProfileType": "Consumption",
                        "name": "Consumption",
                        "enableFips": false
                    }
                ],
                "infrastructureResourceGroup": "cvat-app-infra-rg",
                "peerAuthentication": {
                    "mtls": {
                        "enabled": false
                    }
                },
                "peerTrafficConfiguration": {
                    "encryption": {
                        "enabled": false
                    }
                },
                "publicNetworkAccess": "Disabled"
            }
        },
        {
            "type": "Microsoft.App/managedEnvironments/storages",
            "apiVersion": "2025-02-02-preview",
            "name": "[concat(parameters('managedEnvironments_cvat_test_env_name'), '/cvat-cache-db')]",
            "dependsOn": [
                "[resourceId('Microsoft.App/managedEnvironments', parameters('managedEnvironments_cvat_test_env_name'))]"
            ],
            "properties": {
                "azureFile": {
                    "accountName": "cvatfileshare",
                    "shareName": "cvat-cache-db",
                    "accessMode": "ReadWrite",
                    "accountKeyVaultProperties": {
                        "keyVaultUrl": "https://cvatkv.vault.azure.net/secrets/common-password/9dcb52a1144c474a8a4d41d32464e9f7",
                        "identity": "system"
                    }
                }
            }
        },
        {
            "type": "Microsoft.App/managedEnvironments/storages",
            "apiVersion": "2025-02-02-preview",
            "name": "[concat(parameters('managedEnvironments_cvat_test_env_name'), '/cvat-vector-component')]",
            "dependsOn": [
                "[resourceId('Microsoft.App/managedEnvironments', parameters('managedEnvironments_cvat_test_env_name'))]"
            ],
            "properties": {
                "azureFile": {
                    "accountName": "cvatfileshare",
                    "shareName": "cvat-vector-component",
                    "accessMode": "ReadWrite",
                    "accountKeyVaultProperties": {
                        "keyVaultUrl": "https://cvatkv.vault.azure.net/secrets/common-password/9dcb52a1144c474a8a4d41d32464e9f7",
                        "identity": "system"
                    }
                }
            }
        },
        {
            "type": "Microsoft.App/managedEnvironments/storages",
            "apiVersion": "2025-02-02-preview",
            "name": "[concat(parameters('managedEnvironments_cvat_test_env_name'), '/cvat-vector-tmpfs')]",
            "dependsOn": [
                "[resourceId('Microsoft.App/managedEnvironments', parameters('managedEnvironments_cvat_test_env_name'))]"
            ],
            "properties": {
                "azureFile": {
                    "accountName": "cvatfileshare",
                    "shareName": "cvat-vector-tmpfs",
                    "accessMode": "ReadWrite",
                    "accountKeyVaultProperties": {
                        "keyVaultUrl": "https://cvatkv.vault.azure.net/secrets/common-password/9dcb52a1144c474a8a4d41d32464e9f7",
                        "identity": "system"
                    }
                }
            }
        }
    ]
}