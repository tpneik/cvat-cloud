{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "managedEnvironments_cvat_test_env_name": {
            "defaultValue": "cvat-test-env",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.App/managedEnvironments",
            "apiVersion": "2025-02-02-preview",
            "name": "[parameters('managedEnvironments_cvat_test_env_name')]",
            "location": "Southeast Asia",
            "properties": {
                "appLogsConfiguration": {},
                "zoneRedundant": false,
                "kedaConfiguration": {},
                "daprConfiguration": {},
                "customDomainConfiguration": {},
                "workloadProfiles": [
                    {
                        "workloadProfileType": "Consumption",
                        "name": "Consumption",
                        "enableFips": false
                    }
                ],
                "peerAuthentication": {
                    "mtls": {
                        "enabled": false
                    }
                },
                "peerTrafficConfiguration": {
                    "encryption": {
                        "enabled": false
                    }
                },
                "publicNetworkAccess": "Enabled"
            }
        },
        {
            "type": "Microsoft.App/managedEnvironments/storages",
            "apiVersion": "2025-02-02-preview",
            "name": "[concat(parameters('managedEnvironments_cvat_test_env_name'), '/cvat-cache-db')]",
            "dependsOn": [
                "[resourceId('Microsoft.App/managedEnvironments', parameters('managedEnvironments_cvat_test_env_name'))]"
            ],
            "properties": {
                "azureFile": {
                    "accountName": "cvatfileshare",
                    "shareName": "cvat-cache-db",
                    "accessMode": "ReadWrite"
                }
            }
        },
        {
            "type": "Microsoft.App/managedEnvironments/storages",
            "apiVersion": "2025-02-02-preview",
            "name": "[concat(parameters('managedEnvironments_cvat_test_env_name'), '/cvat-vector-component')]",
            "dependsOn": [
                "[resourceId('Microsoft.App/managedEnvironments', parameters('managedEnvironments_cvat_test_env_name'))]"
            ],
            "properties": {
                "azureFile": {
                    "accountName": "cvatfileshare",
                    "shareName": "cvat-vector-component",
                    "accessMode": "ReadWrite"
                }
            }
        },
        {
            "type": "Microsoft.App/managedEnvironments/storages",
            "apiVersion": "2025-02-02-preview",
            "name": "[concat(parameters('managedEnvironments_cvat_test_env_name'), '/cvat-vector-tmpfs')]",
            "dependsOn": [
                "[resourceId('Microsoft.App/managedEnvironments', parameters('managedEnvironments_cvat_test_env_name'))]"
            ],
            "properties": {
                "azureFile": {
                    "accountName": "cvatfileshare",
                    "shareName": "cvat-vector-tmpfs",
                    "accessMode": "ReadWrite"
                }
            }
        }
    ]
}